<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="application/xml" href="unit.test.xsl"?>

<unit-test title="ut:ends_with">

  <assert-equal name="ut:ends_with">
   <string></string>
   <suffix></suffix>
   <expect>true</expect>
  </assert-equal>

  <assert-equal name="ut:ends_with">
   <string>ABC</string>
   <suffix></suffix>
   <expect>true</expect>
  </assert-equal>

  <assert-equal name="ut:ends_with">
   <string>ABC</string>
   <suffix>C</suffix>
   <expect>true</expect>
  </assert-equal>

  <assert-equal name="ut:ends_with">
   <string>ABC</string>
   <suffix>BC</suffix>
   <expect>true</expect>
  </assert-equal>

  <assert-equal name="ut:ends_with">
   <string>ABC</string>
   <suffix>ABC</suffix>
   <expect>true</expect>
  </assert-equal>

  <assert-equal name="ut:ends_with">
   <string>ABC</string>
   <suffix>X</suffix>
   <expect></expect>
  </assert-equal>

</unit-test>
